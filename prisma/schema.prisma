generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  createdAt DateTime @default(now())
}

model User {
  id    Int    @id @default(autoincrement())
  auth0Id   String @unique
  name  String
  email String @unique
}
